{
	"manifest_version": 2,
	"name": "Chrome Backspace",
	"description": "Chrome backspace key down.\nYou are browser history back",
	"version": "1.0.2",
	"icons": {
		"128": "backspaceImage.png"
	},
	"homepage_url": "http://www.w-here.com",
  	"permissions": [
    	"https://*/*",
    	"http://*/*"
  		],
  	"content_scripts": [
	    {
      		"matches": [
        		"https://*/*",
        		"http://*/*"
      		],
      		"js": [
        		"setBackSpaceKeyDown.js"
      		]
    	}
  	]
}